{
    "name": "web",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "nx serve",
        "build": "ng build sck-app",
        "build:nx": "nx build sck-app",
        "watch": "nx build --watch --configuration development",
        "test": "nx test",
        "prettier:check": "prettier --config .prettierrc --check \"projects/**/*.{ts,css,html}\"",
        "prettier:fix": "prettier --config .prettierrc --write \"projects/**/*.{ts,css,html}\"",
        "prepare": "cd ../.. && husky install src/web/.husky",
        "lint": "nx lint"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run test"
        }
    },
    "lint-staged": {
        "projects/**/*.{ts,scss,html}": [
            "npm run prettier:fix"
        ]
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.0.3",
        "@angular/cdk": "^16.1.0",
        "@angular/common": "^17.0.3",
        "@angular/compiler": "^17.0.3",
        "@angular/core": "^17.0.3",
        "@angular/forms": "^17.0.3",
        "@angular/material": "^16.1.0",
        "@angular/platform-browser": "^17.0.3",
        "@angular/platform-browser-dynamic": "^17.0.3",
        "@angular/router": "^17.0.3",
        "date-fns": "^2.30.0",
        "rxjs": "~7.8.0",
        "showdown": "^2.1.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.2"
    }
}
